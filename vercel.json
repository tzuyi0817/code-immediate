{
  "rewrites": [
    {
      "source": "/css.worker.:path(\\d+).js",
      "destination": "/assets/css.worker.:path(\\d+).js"
    },
    {
      "source": "/html.worker.:path(\\d+).js",
      "destination": "/assets/html.worker.:path(\\d+).js"
    },
    {
      "source": "/ts.worker.:path(\\d+).js",
      "destination": "/assets/ts.worker.:path(\\d+).js"
    },
    {
      "source": "/editor.worker.:path(\\d+).js",
      "destination": "/assets/editor.worker.:path(\\d+).js"
    }
  ]
}