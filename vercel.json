{
  "rewrites": [
    {
      "source": "/css.worker.(\\d+).js",
      "destination": "/assets/css.worker.(\\d+).js"
    },
    {
      "source": "/html.worker.(\\d+).js",
      "destination": "/assets/html.worker.(\\d+).js"
    },
    {
      "source": "/ts.worker.(\\d+).js",
      "destination": "/assets/ts.worker.(\\d+).js"
    },
    {
      "source": "/editor.worker.(\\d+).js",
      "destination": "/assets/editor.worker.(\\d+).js"
    }
  ]
}