{
  "name": "code-immediate",
  "private": true,
  "version": "0.0.0",
  "description": "A code online editing preview tool, similar to codepen",
  "type": "module",
  "license": "MIT",
  "repository": {},
  "keywords": [
    "vue3",
    "typescript",
    "code"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint --ext .js,.ts,.vue",
    "lint:fix": "pnpm run lint --fix .",
    "ts-check": "bash -c vue-tsc --noEmit",
    "test:unit": "vitest --coverage",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "algoliasearch": "^4.14.2",
    "axios": "^1.1.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "loadjs": "^4.2.0",
    "monaco-editor": "^0.47.0",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "pinia": "^2.0.17",
    "pinia-plugin-persistedstate": "^3.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^8.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/loadjs": "^4.0.1",
    "@types/node": "^20.0.0",
    "@types/testing-library__jest-dom": "^5.14.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@volar/monaco": "^2.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/vue3-jest": "^29.2.4",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.57.0",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-vue": "^9.22.0",
    "fs-extra": "^11.0.0",
    "jsdom": "^24.0.0",
    "msw": "^2.0.0",
    "parse-package-name": "^1.0.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.0",
    "postcss-nested": "^6.0.0",
    "postcss-nesting": "^12.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^5.4.2",
    "vite": "^5.0.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vitest": "^1.0.0",
    "vitest-canvas-mock": "^0.3.2",
    "vscode-theme-to-monaco-theme-node": "^1.0.0",
    "vue-tsc": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "*.{ts,js,vue}": [
      "pnpm lint"
    ]
  },
  "optionalDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  }
}
